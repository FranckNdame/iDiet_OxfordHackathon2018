    pod 'Firebase/Storage'
    pod 'MKRingProgressView'
    pod 'Kingfisher'


  target 'iDietTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iDietUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
